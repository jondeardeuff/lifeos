{
  "name": "@lifeos/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:simple": "tsx watch src/server-simple.ts",
    "build": "tsc src/server-simple.ts --outDir dist --target es2020 --module commonjs --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck",
    "start": "node dist/server-simple.js",
    "test": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/server": "^4.9.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.0",
    "@fastify/jwt": "^7.2.0",
    "@fastify/rate-limit": "^8.0.0",
    "@lifeos/database": "workspace:*",
    "@lifeos/types": "workspace:*",
    "@supabase/supabase-js": "^2.38.0",
    "argon2": "^0.31.0",
    "fastify": "^4.24.0",
    "graphql": "^16.8.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@lifeos/eslint-config": "workspace:*",
    "@lifeos/tsconfig": "workspace:*",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}